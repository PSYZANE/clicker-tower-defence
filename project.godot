; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="clicker tower defence"
config/version="0.0.1(test)"
run/main_scene="res://world/world.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://Autoloads/globals.gd"
Path="*res://Autoloads/paths.gd"

[display]

window/size/viewport_width=600
window/size/viewport_height=350
window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[internationalization]

locale/locale_filter_mode=1

[layer_names]

2d_physics/layer_1="player"
2d_physics/layer_2="enemy"
2d_physics/layer_3="items"
2d_physics/layer_4="bullets"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
